    "postcss-import": "^9.0.0",
    "postcss-loader": "^1.2.1",
    "postcss-reporter": "^3.0.0",
    "react": "^15.4.1",
    "react-dom": "^15.4.1",
    "react-helmet": "^3.2.2",
    "react-onclickoutside": "^5.10.0",
    "react-redux": "^4.4.5",
    "react-router": "^2.8.1",
    "react-router-redux": "^4.0.0",
    "react-tap-event-plugin": "^2.0.1",
    "react-transform-hmr": "^1.0.4",
    "redux": "^3.6.0",
    "redux-logger": "^2.6.1",
    "redux-thunk": "^2.0.1",
    "request": "^2.81.0",
    "rimraf": "^2.5.4",
    "sequelize": "^3.24.1",
    "sequelize-cli": "^2.3.1",
    "sharp": "^0.17.3",
    "spark-md5": "^2.0.2",
    "style-loader": "^0.13.0",
