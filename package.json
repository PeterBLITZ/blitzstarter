    "koa-generic-session-mongo": "^0.3.1",
    "koa-passport": "^3.0.0",
    "koa-router": "^7.1.1",
    "koa-static": "^3.0.0",
    "koa-webpack": "^0.3.1",
    "koa-webpack-middleware": "^1.0.3",
