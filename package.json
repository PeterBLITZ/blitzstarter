{
  "name": "blitzstarter",
  "version": "0.1.0",
  "main": "index.js",
  "scripts": {
    "lint": "eslint --ext .js,.jsx app server",
    "lint:fix": "eslint --ext .js,.jsx app server --fix",
    "clean": "rimraf public/assets && rimraf compiled",
    "start": "npm run build && cross-env NODE_ENV=production node compiled/server.js",
    "dev": "npm run build:dev && cross-env NODE_ENV=development node compiled/server.dev.js",
    "build:dev": "npm run clean && cross-env NODE_ENV=development webpack --debug --colors --display-error-details --config ./webpack/webpack.config.js",
    "build": "npm run clean && cross-env NODE_ENV=production webpack --colors --display-error-details --config ./webpack/webpack.config.js"
  },
  "license": "MIT",
  "babel": {
    "presets": [
      [
        "env",
        {
          "targets": {
            "node": "current"
          }
        }
      ],
      [
        "react"
      ]
    ],
    "plugins": [
      [
        "transform-runtime",
        {
          "polyfill": false,
          "regenerator": true
        }
      ]
    ]
  },
  "eslintConfig": {
    "extends": [
      "airbnb",
      "prettier",
      "prettier/react"
    ],
    "env": {
      "browser": true,
      "node": true
    },
    "parser": "babel-eslint",
    "parserOptions": {
      "ecmaVersion": 6,
      "sourceType": "module",
      "ecmaFeatures": {
        "forOf": true,
        "jsx": true,
        "es6": true,
        "experimentalObjectRestSpread": true
      }
    },
    "rules": {
      "quotes": [
        2,
        "single"
      ],
      "jsx-quotes": [
        2,
        "prefer-single"
      ],
      "comma-dangle": [
        "error",
        {
          "arrays": "always-multiline",
          "objects": "always-multiline",
          "imports": "always-multiline",
          "exports": "always-multiline",
          "functions": "never"
        }
      ]
    },
    "plugins": [
      "react",
      "prettier"
    ]
  },
  "devDependencies": {
    "babel-eslint": "^6.1.2",
    "babel-plugin-transform-runtime": "^6.23.0",
    "babel-register": "^6.24.0",
    "enzyme": "^2.4.1",
    "eslint": "^3.19.0",
    "eslint-config-airbnb": "^10.0.1",
    "eslint-config-prettier": "^1.5.0",
    "eslint-loader": "^1.7.1",
    "eslint-plugin-import": "^1.16.0",
    "eslint-plugin-jsx-a11y": "^2.2.3",
    "eslint-plugin-prettier": "^2.0.1",
    "eslint-plugin-react": "^6.10.3",
    "expect": "^1.15.2",
    "jsdom": "^9.12.0",
    "mocha": "^3.2.0",
    "nodemon": "^1.10.2",
    "prettier": "^0.22.0",
    "react-addons-test-utils": "^15.4.1",
    "react-transform-catch-errors": "^1.0.2",
    "redux-mock-store": "1.1.2",
    "sinon": "^1.17.5"
  },
  "engines": {
    "node": ">=7.x"
  },
  "dependencies": {
    "async": "^2.3.0",
    "autoprefixer": "^6.4.0",
    "aws-sdk": "^2.41.0",
    "axios": "^0.14.0",
    "babel-cli": "^6.24.0",
    "babel-core": "^6.24.0",
    "babel-eslint": "^6.1.2",
    "babel-loader": "^6.4.1",
    "babel-plugin-react-transform": "^2.0.2",
    "babel-plugin-transform-react-constant-elements": "^6.23.0",
    "babel-plugin-transform-react-inline-elements": "^6.22.0",
    "babel-plugin-transform-react-remove-prop-types": "^0.2.12",
    "babel-polyfill": "^6.23.0",
    "babel-preset-env": "^1.3.2",
    "babel-preset-es2015": "^6.24.0",
    "babel-preset-react": "^6.23.0",
    "babel-preset-react-hmre": "^1.1.1",
    "babel-preset-stage-0": "^6.22.0",
    "babel-regenerator-runtime": "^6.5.0",
    "babel-runtime": "^6.23.0",
    "bcrypt-nodejs": "0.0.3",
    "bluebird": "^3.4.6",
    "body-parser": "^1.15.0",
    "classnames": "^2.2.3",
    "compression": "^1.6.2",
    "connect-mongo": "^1.3.2",
    "connect-pg-simple": "^3.1.0",
    "cookie-parser": "^1.4.3",
    "cross-env": "^2.0.1",
