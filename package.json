{
  "name": "blitzstarter",
  "version": "0.0.2",
  "main": "server.js",
  "scripts": {
    "start": "npm run start:prod:manual",
    "start:prod:manual": "npm run clean && npm run build:prod && NODE_PATH=. NODE_ENV=production node server.js",
    "start:dev:manual": "npm run clean && npm run build:dev && NODE_PATH=. NODE_ENV=development node server.js",
    "clean": "rimraf public/assets",
    "build:prod": "NODE_ENV=production NODE_PATH=. webpack --colors --config ./webpack/webpack.config.prod.js",
    "build:dev": "NODE_ENV=development  NODE_PATH=. webpack --colors --config ./webpack/webpack.config.dev-server.js",
    "build": "npm run clean && npm run build:prod",
    "test": "echo \"Error: no test specified\" && exit 1"
  },
  "engines": {
    "node": "7.x.x"
  },
  "babel": {
    "presets": [
      "env",
      "react"
    ]
  },
  "eslintConfig": {
    "extends": [
      "airbnb",
      "prettier",
      "prettier/react"
    ],
    "parser": "babel-eslint",
    "parserOptions": {
      "sourceType": "module",
      "ecmaFeatures": {
        "experimentalObjectRestSpread": true,
        "jsx": true
      }
    },
    "env": {
      "browser": true,
      "node": true,
      "es6": true
    },
    "plugins": [
      "react",
      "prettier"
    ],
    "rules": {
      "quotes": [
        2,
        "single"
      ],
      "jsx-quotes": [
        2,
        "prefer-single"
      ],
      "no-underscore-dangle": 0,
      "comma-dangle": [
        "error",
        {
          "arrays": "always-multiline",
          "objects": "always-multiline",
          "imports": "always-multiline",
          "exports": "always-multiline",
          "functions": "never"
        }
      ]
    },
    "globals": {
      "__DEVSERVER__": true,
      "__DEVCLIENT__": true,
      "__CSR__": false
    }
  },
  "license": "MIT",
  "dependencies": {
    "async": "^2.2.0",
    "axios": "^0.15.3",
    "babel-polyfill": "^6.23.0",
    "babel-runtime": "^6.23.0",
    "body-parser": "^1.17.1",
    "colors": "^1.1.2",
    "connect-mongo": "^1.3.2",
    "cookie-parser": "^1.4.3",
    "errorhandler": "^1.5.0",
    "express": "^4.15.2",
    "express-session": "^1.15.2",
    "file-type": "^4.1.0",
    "htmlparser2": "^3.9.2",
    "lodash": "^4.17.4",
    "logdown": "^2.2.0",
    "material-ui": "^0.17.1",
    "method-override": "^2.3.8",
    "mongodb": "^2.2.25",
    "mongoose": "^4.9.2",
    "morgan": "^1.8.1",
    "multer": "^1.3.0",
    "nconf": "^0.8.4",
    "passport": "^0.3.2",
    "passport-facebook": "^2.1.1",
    "passport-local": "^1.0.0",
    "react": "^15.4.2",
    "react-dom": "^15.4.2",
    "react-masonry-layout": "^0.2.3",
    "react-onclickoutside": "^5.10.0",
    "react-redux": "^5.0.3",
    "react-router": "^3.0.2",
    "react-router-redux": "^4.0.8",
    "react-tap-event-plugin": "^2.0.1",
    "redux": "^3.6.0",
    "redux-auth-wrapper": "^1.0.0",
    "redux-logger": "^2.10.2",
    "redux-thunk": "^2.2.0",
    "request": "^2.81.0",
    "ua-parser-js": "^0.7.12",
    "validator": "^7.0.0",
    "winston": "^2.3.1"
  },
  "devDependencies": {
    "autoprefixer": "^6.7.7",
    "babel-cli": "^6.24.0",
    "babel-core": "^6.24.0",
    "babel-eslint": "^7.2.1",
    "babel-loader": "^6.4.1",
    "babel-plugin-lodash": "^3.2.11",
    "babel-plugin-react-transform": "^2.0.2",
    "babel-plugin-transform-decorators-legacy": "^1.3.4",
    "babel-plugin-transform-object-assign": "^6.22.0",
    "babel-plugin-transform-react-constant-elements": "^6.23.0",
    "babel-plugin-transform-react-inline-elements": "^6.22.0",
    "babel-plugin-transform-react-remove-prop-types": "^0.3.2",
    "babel-plugin-transform-runtime": "^6.23.0",
    "babel-preset-env": "^1.2.2",
    "babel-preset-es2015": "^6.24.0",
    "babel-preset-react": "^6.23.0",
    "babel-preset-react-hmre": "^1.1.1",
    "babel-preset-stage-0": "^6.22.0",
    "css-loader": "^0.27.3",
    "eslint": "^3.18.0",
    "eslint-config-airbnb": "^14.1.0",
    "eslint-config-prettier": "^1.5.0",
    "eslint-loader": "^1.7.0",
    "eslint-plugin-import": "^2.2.0",
    "eslint-plugin-jsx-a11y": "^4.0.0",
    "eslint-plugin-prettier": "^2.0.1",
    "eslint-plugin-react": "^6.10.3",
    "extract-text-webpack-plugin": "^1.0.1",
    "inline-environment-variables-webpack-plugin": "^1.1.0",
    "json-loader": "^0.5.4",
    "node-sass": "^4.5.1",
    "postcss-loader": "^1.3.3",
    "precss": "^1.4.0",
    "prettier": "^0.22.0",
    "react-hot-loader": "^1.3.1",
    "redux-devtools": "^3.3.2",
    "redux-devtools-dock-monitor": "^1.1.1",
    "redux-devtools-log-monitor": "^1.2.0",
    "rimraf": "^2.6.1",
    "sass-loader": "^4.1.1",
    "strip-loader": "^0.1.2",
    "style-loader": "^0.16.0",
    "webpack": "^1.14.0",
    "webpack-dev-middleware": "^1.10.1",
    "webpack-hot-middleware": "^2.17.1"
  }
}
